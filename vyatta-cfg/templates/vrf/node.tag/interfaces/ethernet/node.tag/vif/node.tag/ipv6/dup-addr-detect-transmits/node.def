type: u32
help: Number of NS messages to send while performing DAD

val_help: <1-N>; Number of NS messages to send while performing DAD
val_help: 0; Disable Duplicate Address Dectection (DAD)

default: 1

syntax:expression: ($VAR(@) >= 0) ; "Value must be >= 0"

update:
        procfile=/proc/sys/net/ipv6/conf/$VAR(../../../@).$VAR(../../@)/dad_transmits
        if sudo /sbin/ip netns exec $VAR(../../../../../@) [ -e $procfile ]; then
            sudo /sbin/ip netns exec $VAR(../../../../../@) sh -c "echo $VAR(@) > $procfile"
        else
            echo "Will set dup_addr_detect_transmits when $VAR(../../../@).$VAR(../../@) comes up"
        fi
